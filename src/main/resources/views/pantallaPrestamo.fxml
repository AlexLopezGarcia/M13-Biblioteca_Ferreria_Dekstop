<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="cat.ferreria.dekstop.controller.PrestamoController"
            prefHeight="200.0" prefWidth="400.0">

    <children>
        <Label layoutX="30" layoutY="30" text="ID del libro:" />
        <TextField fx:id="libroIdField" layoutX="120" layoutY="25" prefWidth="200"/>

        <Button fx:id="btnPrestar" layoutX="50" layoutY="80" text="Prestar"/>
        <Button fx:id="btnDevolver" layoutX="180" layoutY="80" text="Devolver"/>

        <Label fx:id="mensajeLabel" layoutX="30" layoutY="130" prefWidth="340"/>
    </children>
</AnchorPane>
